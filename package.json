{
  "name": "typescript-discord-bot",
  "version": "1.0.0",
  "description": "Typescript Discord Bot template using discord.js v14",
  "main": "dist/app/index.js",
  "engines": {
    "node": "^20.10.0"
  },
  "scripts": {
    "build": "tsc && babel dist --out-dir dist",
    "dev": "pnpm cleanup && pnpm build && node --enable-source-maps .",
