  "license": "MIT",
  "type": "module",
  "dependencies": {
    "chalk": "^5.3.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "typescript": "^5.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@types/node": "20.10.0",
    "babel-plugin-root-import": "^6.6.0",
    "prettier": "3.2.5"
  }
}
